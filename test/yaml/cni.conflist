{
	"cniVersion": "0.3.1",
	"name": "k8s-pod-network",
	"plugins": [
	{
		"type": "ptp",
		"ipMasq": false,
		"ipam": {
			"type": "host-local",
			"dataDir": "/run/cni-ptp<<NAME>>-ipam-state",
			"routes": [
			      { "dst": "0.0.0.0/0" },{ "dst": "::/0" }
            ],
			"ranges": [
                <<IP_RANGE>>
			]
		},
		"mtu": 1500
	},
	{
		"type": "portmap",
		"capabilities": {
			"portMappings": true
		}
	}
	]
}